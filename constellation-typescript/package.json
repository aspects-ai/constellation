{
  "name": "constellationfs",
  "version": "0.1.0",
  "description": "A filesystem abstraction for AI agents that provides familiar bash commands instead of custom APIs",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "constellationfs": "./bin/constellationfs"
  },
  "files": [
    "dist",
    "native",
    "scripts",
    "cli",
    "bin",
    "remote"
  ],
  "scripts": {
    "build": "vite build",
    "build:native": "node scripts/build-native.js",
    "dev": "vite build --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run build:native"
  },
  "keywords": [
    "filesystem",
    "ai",
    "agents",
    "bash",
    "typescript",
    "tools"
  ],
  "author": "ConstellationFS Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/constellation-fs/constellation-fs.git"
  },
  "bugs": {
    "url": "https://github.com/constellation-fs/constellation-fs/issues"
  },
  "homepage": "https://github.com/constellation-fs/constellation-fs#readme",
  "dependencies": {
    "@codebuff/sdk": "^0.1.33",
    "ssh2": "^1.16.0",
    "zod": "^4.1.5"
  },
  "optionalDependencies": {
    "node-gyp": "^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "@types/ssh2": "^1.15.5",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.16.0",
    "prettier": "^3.4.1",
    "typescript": "^5.7.2",
    "vite": "^6.0.1",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "directories": {
    "example": "examples",
    "test": "tests"
  }
}
