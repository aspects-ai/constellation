{
  "description": "Template configuration for this project. See https://www.codebuff.com/config for all options.",
  "startupProcesses": [
    {
      "name": "web-demo",
      "command": "npm run dev",
      "cwd": "examples/web-demo",
      "stdoutFile": "logs/web-demo.log"
    }
  ],
  "fileChangeHooks": [
    {
      "name": "typecheck-web-demo",
      "command": "npx tsc --noEmit",
      "cwd": "examples/web-demo",
      "filePattern": "examples/web-demo/app/*.{ts,tsx}|examples/web-demo/lib/*.{ts,tsx}",
      "enabled": true
    }
  ]
}
